{
  "@jupyterlab/notebook-extension:tracker": {
    "kernelShutdown": true,
    "codeCellConfig": {
      "autoClosingBrackets": true,
      "fontFamily": null,
      "fontSize": null,
      "lineHeight": null,
      "lineNumbers": true,
      "lineWrap": "off",
      "matchBrackets": true,
      "readOnly": false,
      "insertSpaces": true,
      "tabSize": 4,
      "wordWrapColumn": 80,
      "rulers": [],
      "codeFolding": true,
      "lineWiseCopyCut": true
    },
  },
  "@jupyterlab/filebrowser-extension:browser": {
    "navigateToCurrentDirectory": true,
    "useFuzzyFilter": true
  },
  "@jupyterlab/extensionmanager-extension:plugin": {
    "enabled": false
  },
  "@ijmbarr/jupyterlab_spellchecker:plugin": {
    "language": "de-ch"
  }
}